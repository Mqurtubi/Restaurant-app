{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 15,
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": "off",
    "no-console": "off",
    "class-methods-use-this": "off",
    "import/no-unresolved": "off",
    "consistent-return": "off",
    "import/no-extraneous-dependencies": "off",
    "import/extensions": "off",
    "max-len": "off",
    "func-names": "off",
    "import/prefer-default-export": "off",
    "no-undef": "off"
  }
}
